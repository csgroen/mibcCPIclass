glm_model100 <- readRDS("Results/glm_model100.RDS")
model <- glm_model100$finalModel
feats <- readRDS("Results/features100.RDS")

library(TCGAbiolinks)
#-- Download the TCGA-LIHC data
query <- GDCquery("TCGA-BLCA", 
                  data.category = "Transcriptome Profiling", 
                  data.type = "Gene Expression Quantification",
                  workflow.type = "HTSeq - FPKM")
GDCdownload(query)
tcga_blca <- GDCprepare(query)
